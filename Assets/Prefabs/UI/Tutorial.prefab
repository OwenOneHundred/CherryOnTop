  m_Script: {fileID: 11500000, guid: f8fced82afe7d45adb329f3ed7837666, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  text: {fileID: 9010367111406043723}
  charactersPerSecond: 60
  welcome:
    location: {x: 0, y: 0, z: 0}
    text: Welcome to Cherry on Top!
    textSize: 40
  gameSummary:
    location: {x: 0, y: 0, z: 0}
    text: Here's your cake! You need to keep cherries from reaching the top, or there's
      a "cherry on top," so your cake - and your run - are done!
    textSize: 35
  whatAreToppings:
    location: {x: 0, y: 0, z: 0}
    text: We need some toppings to defend our cake from cherries.
    textSize: 40
  openTheShop:
    location: {x: 0, y: 0, z: 0}
    text: Click the shop button on the left to open the shop.
    textSize: 40
  readDescriptionsAndBuySomething:
    location: {x: 0, y: 0, z: 0}
    text: Hover items in the shop to read what they do. Toppings have effects that
      are active when they're placed! Click a few to buy them.
    textSize: 35
  onceSatisfiedCloseShop:
    location: {x: 0, y: 0, z: 0}
    text: You can reroll the options with the button at the bottom. Once you've bought
      enough toppings for now, close the shop by pressing the shop button again.
    textSize: 31
  dragToppingsOntoCake:
    location: {x: 0, y: 0, z: 0}
    text: Now drag the toppings from your inventory on the right side of the screen
      onto the cake! Most toppings attack cherries that are in the range shown.
    textSize: 32
  startWhenReady:
    location: {x: 0, y: 0, z: 0}
    text: Once you're ready, press start on the left side of the screen. If you beat
      Round 30, you win! Good luck!
    textSize: 35
